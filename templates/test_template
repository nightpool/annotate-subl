get '/auth' do
    code = [params[:code]]()
    erb install, locals: {token: get_token(code)}
end

def find_git(file_path):
    git_path = None
    for path, folders, _ in [utils.walk_up]()(os.path.dirname(file_path)):
        if '.git' in folders:
            git_path = [os.path.join(path, '.git')]()
            break
    return git_path
